ent:PropTypes.func.isRequired\r\n    }\r\n    render() {\r\n        const {comments,delComment} = this.props\r\n        const display = comments.length == 0 ? 'block' : 'none'\r\n        return (\r\n            <div className=\"col-md-8\">\r\n                <h3 className=\"reply\">评论回复：</h3>\r\n                <h2 style={{ display: display }}>暂无评论，点击左侧添加评论！！！</h2>\r\n                <ul className=\"list-group\">\r\n                    {\r\n                        comments.map((item,index)=> <CommentItem comments={item} key={index} delComment={delComment} index={index}/>)\r\n                    }\r\n                </ul>\r\n            </div>\r\n        );\r\n    }\r\n\r\n}\r\n// commentList.protoTypes = {\r\n//     comments:ProtoTypes.array.isRequired\r\n// }\r\nexport default commentList;\r\n","E:\\暂时用可删除\\新建文件夹\\code\\react_app\\src\\components\\comment-item\\commentItem.jsx",["59","60"],"import React, { Component } from 'react';\r\nimport PropTypes from 'prop-types'\r\n\r\nimport './commentItem.css'\r\nclass commentItem extends Component {\r\n    static propTypes = {\r\n        comments:PropTypes.object.isRequired,\r\n        delComment:PropTypes.func.isRequired,\r\n        index:PropTypes.number.isRequired,\r\n    }\r\n\r\n    delItem=()=>{\r\n        const {comments,delComment,index}=this.props\r\n        if(window.confirm(`确定删除${comments.username}的评论吗？`)){\r\n            delComment(index)\r\n        }\r\n    }\r\n    render() {\r\n        const {comments} = this.props\r\n        return (\r\n            <li className=\"list-group-item\" >\r\n                <div className=\"handle\">\r\n                    <a href=\"javascript:;\" onClick={this.delItem}>删除</a>\r\n                </div>\r\n                <p className=\"user\"><span >{comments.username}</span><span>说:</span></p>\r\n                <p className=\"centence\">{comments.content}</p>\r\n            </li>\r\n        );\r\n    }\r\n}\r\n\r\nexport default commentItem;\r\n","E:\\暂时用可删除\\新建文件夹\\code\\react_app\\src\\components\\app1\\app1.jsx",[],["61","62"],"E:\\暂时用可删除\\新建文件夹\\code\\react_app\\src\\components\\app1\\Search.jsx",[],"E:\\暂时用可删除\\新建文件夹\\code\\react_app\\src\\components\\app1\\Main.jsx",["63","64"],"import React, { Component } from 'react';\r\nimport PropTypes from 'prop-types'\r\nimport axios from 'axios'\r\n\r\nclass Main extends Component {\r\n    static propTypes = {\r\n        searchName: PropTypes.string.isRequired\r\n    }\r\n\r\n    state = {\r\n        initView: true,\r\n        loading: false,\r\n        users: null,\r\n        errorMsg: null\r\n    }\r\n    //当组件接收到新的参数\r\n    componentWillReceiveProps(newProps) {\r\n        const { searchName } = newProps\r\n        this.setState({\r\n            initView: false,\r\n            loading: true\r\n        })\r\n        const url = `https://api.github.com/search/users?q=${searchName}`\r\n        axios.get(url).then(res => {\r\n            const data = res.data\r\n            console.log(data);\r\n            const users = data.items.map(item=>{\r\n                return {name:item.login,url:item.html_url,avatarUrl:item.avatar_url}\r\n            })\r\n            this.setState({loading:false,users})\r\n        }).catch(err => {\r\n            this.setState({loading:false,errorMsg:err.message})\r\n        })\r\n    }\r\n\r\n    render() {\r\n        let { initView, loading, users, errorMsg } = this.state\r\n        let { searchName } = this.props\r\n        if (initView) {\r\n            return <h2>请输入关键字进行搜索:{searchName}</h2>\r\n        } else if (loading) {\r\n            return <h2>正在请求中。。。</h2>\r\n        } else if (errorMsg) {\r\n            return <h2>{errorMsg}</h2>\r\n        } else {\r\n            return (\r\n                <div className=\"row\">\r\n                    {\r\n                        users.map((item, index) => {\r\n                            return (\r\n                                <div className=\"card\" key={index}>\r\n                                    <a href={item.url} target=\"_blank\">\r\n                                        <img src={item.avatarUrl} style={{ width: 100 }} />\r\n                                    </a>\r\n                                    <p className=\"card-text\">{item.name}</p>\r\n                                </div>\r\n                            )\r\n                        })\r\n                    }\r\n                </div>\r\n            );\r\n        }\r\n    }\r\n}\r\n\r\nexport default Main;\r\n","E:\\暂时用可删除\\新建文件夹\\code\\react_app\\src\\containers\\app\\app.jsx",[],"E:\\暂时用可删除\\新建文件夹\\code\\react_app\\src\\redux\\store.js",[],"E:\\暂时用可删除\\新建文件夹\\code\\react_app\\src\\redux\\actions.js",[],"E:\\暂时用可删除\\新建文件夹\\code\\react_app\\src\\redux\\action-types.js",[],"E:\\暂时用可删除\\新建文件夹\\code\\react_app\\src\\redux\\reducers.js",["65","66","67"],"import {ADD_COMMENT,DEL_COMMENT} from './action-types'\r\n\r\nconst initComments=[\r\n    {username:'sss',content:'111'},\r\n    {username:'mmm',content:'222'},\r\n] \r\nexport function comments(state=initComments,action){\r\n    switch(action.type){\r\n        case ADD_COMMENT:\r\n            return [action.data,...state]\r\n            break;\r\n        case DEL_COMMENT:\r\n            return state.filter((comment,index) => index != action.data)    \r\n            break;\r\n        default:\r\n            return state    \r\n    }\r\n}",{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","severity":1,"message":"73","line":15,"column":41,"nodeType":"74","messageId":"75","endLine":15,"endColumn":43},{"ruleId":"76","severity":1,"message":"77","line":23,"column":21,"nodeType":"78","endLine":23,"endColumn":67},{"ruleId":"79","severity":1,"message":"80","line":23,"column":29,"nodeType":"81","messageId":"82","endLine":23,"endColumn":43},{"ruleId":"68","replacedBy":"83"},{"ruleId":"70","replacedBy":"84"},{"ruleId":"85","severity":1,"message":"86","line":52,"column":56,"nodeType":"87","endLine":52,"endColumn":71},{"ruleId":"88